{
  "name": "docs",
  "version": "1.0.0",
  "description": "A example of project docs using docsify, and a tool to automate implementing project docs.",
  "main": "docs",
  "bin": {
    "skan-docs": "skan-docs"
  },
  "scripts": {
    "build": "run-s -s make-bin build-bin mod-bin copy-templates",
    "make-bin": "mkdir -p bin",
    "build-bin": "babel --presets latest src --out-file bin/skan-docs",
    "mod-bin": "chmod +x bin/skan-docs",
    "copy-templates": "cp -R ./src/templates/ ./bin/templates",
    "start": "babel-node --presets latest ./configure.js",
    "docs": "docsify serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skan-io/docs.git"
  },
  "keywords": [
    "Documentation",
    "Docs",
    "Project",
    "Docs",
    "Doc",
    "tool"
  ],
  "author": "Nick Manks",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/skan-io/docs/issues"
  },
  "homepage": "https://github.com/skan-io/docs#readme",
  "dependencies": {
    "ascii-art": "^1.4.3",
    "chalk": "^2.4.1",
    "docsify-cli": "^4.2.1",
    "npm-run-all": "^4.1.3",
    "serve": "^6.5.8"
  },
  "devDependencies": {
    "argparse": "^1.0.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1"
  }
}
